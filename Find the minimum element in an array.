/* Finding minimum element of array */

import java.util.*;
public class HelloWorld{

     public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] array = new int[n];
        int min = array[0];
        for(int i=0;i<n;i++){
            array[i] = sc.nextInt();
             min = array[0];
              if(array[i]<min){
              min=array[i];
              }
            }
          System.out.println(min);
        
     }
     }

/*--------------------------------------------------------------------------*/

import java.util.*;
public class HelloWorld{

     public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] array = new int[n];
        for(int p=0;p<n;p++){
            array[p] = sc.nextInt();
        }
        int min = array[0];
        for(int i=0;i<n;i++){
              if(array[i]<min){
              min=array[i];
              }
            }
          System.out.println(min);
        
     }
     }
/*-----------------------------------------------------------------------------*/
